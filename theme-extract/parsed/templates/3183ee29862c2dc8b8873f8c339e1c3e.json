{"components/common/requireness-msg":"<small>\n    {{#if required}}*{{/if}}\n</small>\n","components/products/options/input-numbers":"<div class=\"form-field\" data-product-attribute=\"input-number\" data-xname=\"{{this.display_name}}\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"attribute_number_{{id}}\">\r\n        {{ this.display_name }}\r\n        {{> components/common/requireness-msg}}\r\n    </label>\r\n\r\n    <input class=\"form-input\" type=\"number\" id=\"attribute_number_{{id}}\" {{#unless this.integer_only}}step=\"0.1\"{{/unless}} name=\"attribute[{{this.id}}]\" value=\"{{this.prefill}}\" {{#if required}}required{{/if}}>\r\n</div>\r\n"}
