{"components/halothemes/home/halo-product-tab-by-category":"<div class=\"halo-block halo-block-product halo-block-product-tabs\" data-halo-load=\"animation\">\r\n    <div class=\"container container-1600\">\r\n        {{#if title}}\r\n            <div class=\"halo-block-header center o-h\">\r\n                <h2 class=\"title d-inline-block halo-fadeInUp\">{{title}}</h2>\r\n            </div>\r\n        {{/if}}\r\n        <div class=\"halo-productbyTabs\">\r\n            <ul class=\"tabs center haloCustomScrollbar-x disable-srollbar m-b-40 m-b-20-tb\" data-tab role=\"tablist\">\r\n                {{#each categories}}\r\n                    {{#if id '==' ../cateID}}\r\n                        <li class=\"tab f-none d-inline-block is-active\" role=\"presentation\">\r\n                            <a class=\"tab-title f-heading b-r-100\" href=\"#category_{{id}}\" role=\"tab\" tabindex=\"0\" aria-selected=\"true\" aria-controls=\"category_{{id}}\">\r\n                                <span>All products</span>\r\n                            </a>\r\n                        </li>\r\n                        {{#if children}}\r\n                            {{#each (limit children 3)}}\r\n                                <li class=\"tab f-none d-inline-block\" role=\"presentation\">\r\n                                    <a class=\"tab-title f-heading b-r-100\" href=\"#category_{{id}}\" role=\"tab\" tabindex=\"0\" aria-selected=\"true\" aria-controls=\"category_{{id}}\">\r\n                                        <span>{{name}}</span>\r\n                                    </a>\r\n                                </li>\r\n                            {{/each}}\r\n                        {{/if}}\r\n                    {{/if}}\r\n                {{/each}}\r\n                <li class=\"tab f-none d-inline-block\" role=\"presentation\">\r\n                            <a class=\"tab-title f-heading b-r-100\" href=\"https://customgenie.com/popular-products/\" role=\"tab\" tabindex=\"0\" aria-selected=\"true\" aria-controls=\"View All\" onclick=\"window.location.href='https://customgenie.com/popular-products/'\">\r\n                                <span>View All</span>\r\n                            </a>\r\n                        </li>\r\n            </ul>\r\n            <div class=\"tabs-contents m-0\">\r\n                {{#each categories}}\r\n                    {{#if id '==' ../cateID}}\r\n                        <div class=\"tab-content is-active\" id=\"category_{{id}}\" role=\"tabpanel\" aria-hidden=\"false\" data-tab-category-id=\"{{id}}\" data-tab-category-url=\"{{url}}\">\r\n                            <div class=\"loadingOverlay\"></div>\r\n                            <section class=\"productCarousel haloSwiper pos-relative o-h\"\r\n                                data-list-name=\"Product By Tabs\"\r\n                                data-per-view-mob=\"{{../theme_settings.cardProd_colMob}}\" \r\n                                data-per-view-tab=\"{{../theme_settings.cardProd_colTab}}\" \r\n                                data-per-view-des=\"{{../theme_settings.cardProd_col}}\" \r\n                                data-swiper-pag-mob=\"true\" \r\n                                data-swiper-space-tab=\"20\" \r\n                                data-swiper-space-des=\"30\"\r\n                                data-rows-des=\"2\"\r\n                            >\r\n                                <div class=\"swiper-wrapper disable-srollbar\">\r\n                                    {{> components/halothemes/product/halo-product-sample}}\r\n                                </div>\r\n                                <div class=\"swiper-pagination\"></div>\r\n                            </section>\r\n                        </div>\r\n                        {{#if children}}\r\n                            {{#each (limit children 3)}}\r\n                                <div class=\"tab-content\" id=\"category_{{id}}\" role=\"tabpanel\" aria-hidden=\"